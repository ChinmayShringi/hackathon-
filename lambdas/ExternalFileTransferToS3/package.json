{
  "name": "external-file-transfer-to-s3",
  "type": "module",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "build": "esbuild index.ts --bundle --platform=node --target=node22 --format=esm --outfile=dist/index.js --external:@aws-sdk/*",
    "test": "node --test dist/**/*.test.js",
    "test:watch": "node --test --watch dist/**/*.test.js",
    "test:coverage": "node --test --coverage dist/**/*.test.js",
    "test:local": "tsx test-runner.ts",
    "deploy": "tsx deploy.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/lib-storage": "^3.500.0",
    "dotenv": "^16.4.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "@types/node": "^22.0.0",
    "esbuild": "^0.20.0"
  }
} 